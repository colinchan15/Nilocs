<p id="notice"><%= notice %></p>

<h1>Cubes</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Description</th>
      <th>Condition</th>
      <th>Finish</th>
      <th>Title</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cubes.each do |cube| %>
      <tr>
        <td><%= cube.brand %></td>
        <td><%= cube.model %></td>
        <td><%= cube.description %></td>
        <td><%= cube.condition %></td>
        <td><%= cube.finish %></td>
        <td><%= cube.title %></td>
        <td><%= cube.price %></td>
        <td><%= link_to 'Show', cube %></td>
        <td><%= link_to 'Edit', edit_cube_path(cube) %></td>
        <td><%= link_to 'Destroy', cube, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cube', new_cube_path %>
